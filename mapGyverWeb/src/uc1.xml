<struts>
	<package name="uc1" namespace="/mapgyver/uc1" extends="struts-default">
		<action name="afficheLogin">
			<result>/vue/login.jsp</result>
		</action>
		<action name="do*" class="webApp.uc1Administrer.AdministrerAction" method="{1}">
			<result name="error">/vue/login.jsp</result>
			<result name="success">/vue/index.jsp</result>
			<allowed-methods>login,logout,forgot</allowed-methods>			
		</action>

		<!-- ================================== 

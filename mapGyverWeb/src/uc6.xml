<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
		"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
		"http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
	<package name="uc6" namespace="/mapgyver/uc6" extends="struts-default">
		<action name="jouer">
			<result>/vue/jouer.jsp</result>
		</action>
<action name="jouer*" class="webApp.uc6Jouer.JouerAction" method="{1}">
			<result name="error">/vue/login.jsp</result>
			<result name="success">/vue/jouer.jsp</result>
			<allowed-methods>repondreElire</allowed-methods>			
		</action>		
	</package>
</struts>
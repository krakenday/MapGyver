<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
		"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
		"http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
	<package name="uc2" namespace="/mapgyver/uc2" extends="struts-default">
		<action name="afficheSouvenir">
			<result>/vue/souvenirs.jsp</result>
		</action>
		<action name="do*" class="webApp.uc2Souvenir.AdministrerSouvenir" method="{1}">
			<result name="error">/vue/login.jsp</result><!-- A modif -->
			<result name="success">/vue/index.jsp</result>
			<allowed-methods>login,logout,forgot</allowed-methods>			
		</action>
	</package>
</struts>